{
  "name": "can-debugger-modular",
  "displayName": "CAN Debugger (Modular)",
  "description": "Mod\u00fcler webview: Speed, RPM, GPS, Fuel/Pressure/FuelRate, Temperature charts + gauges, 3D Vehicle, Live Feed (pretty JSON), status & filters.",
  "version": "0.9.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:canDebugger.open"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "canDebugger.open",
        "title": "CAN Debugger: Open Modular Dashboard"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CAN Debugger",
      "properties": {
        "canDebugger.brokerUrl": {
          "type": "string",
          "default": "mqtt://localhost:1883"
        },
        "canDebugger.topic": {
          "type": "string",
          "default": "can/#"
        },
        "canDebugger.username": {
          "type": "string",
          "default": ""
        },
        "canDebugger.password": {
          "type": "string",
          "default": ""
        },
        "canDebugger.clientId": {
          "type": "string",
          "default": "vscode-can-modular"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node tools/copy-three.js"
  },
  "dependencies": {
    "mqtt": "^4.3.7",
    "three": "^0.160.0"
  }
}